<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cbnu.diary.mapper.MemberMapper">

	<select id="findAll" resultType="com.cbnu.diary.entity.Member">
		SELECT * FROM MEMBER
	</select>

	<select id="findOne" resultType="com.cbnu.diary.entity.Member">
		SELECT * FROM MEMBER WHERE ID = #{id}
	</select>

</mapper>
